QPushButton{
    /*font-family: "Segoe UI";
    background-color: #666666;*/

    background-color: #3498db;
    color: #fff;
    border: none;
    border-radius: 3px;
    padding: 5px 10px;
}

QPushButton:hover{
    /*background-color: #777777;
    border-color:  #444444;*/
    background-color: #2980b9;

}

QPushButton:pressed {
    background-color: #888888;
    border-color: #333333;
    color: white;
}
QPushButton:disabled {
    color: #333333;
}

