#Widget QGroupBox
{
    background-color: #ecf0f1;
    border: 1px solid #bdc3c7;
    border-radius: 5px;
    padding: 5px;
}

#groupBox_2 QRadioButton
{

    spacing: 2px;
}

#groupBox_2 QRadioButton::indicator{
    width: 25px;
    height: 25px;
}

#groupBox_2 QRadioButton::indicator:unchecked {
    image: url(:/image/radiobutton_unchecked.png);
}

#groupBox_2 QRadioButton::indicator::checked {
    image: url(:/image/radiobutton_checked.png);
}


#groupBox_3 QCheckBox{
    spacing: 8px;
}
#groupBox_3 QCheckBox::indicator{
    width: 60px;
    height: 60px;
}
#groupBox_3 QCheckBox::indicator:unchecked{
    image: url(:/image/checkbox_unchecked.png);
}
#groupBox_3 QCheckBox::indicator:checked {
    image: url(:/image/checkbox_checked.png);
}

